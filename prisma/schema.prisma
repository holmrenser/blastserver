// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearch"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model blastjob {
  id         String    @id @unique
  parameters Json
  submitted  DateTime
  finished   DateTime?
  results    String?   @db.Xml
  log        String?
  err        String?
}

model taxonomy {
  id        String @id @unique
  name      String
  ancestors String
}
