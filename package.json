{
  "name": "blastserver",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= v18.17.0"
  },
  "scripts": {
    "dev:app": "ENV_FILE='./.env.development' next dev",
    "build:app:dev": "rm -rf .next && ENV_FILE='./.env.development' next build && ls -lah .next && cp -r .next/static .next/standalone/.next/static",
    "build:app:prod": "rm -rf .next && ENV_FILE='./.env.production' next build && cp -r .next/static .next/standalone/.next/static",
    "build:worker": "rm -rf worker/build && npx tsc -p tsconfig.worker.json",
    "start:app": "node -r dotenv/config app.js",
    "dev:worker": "run-p dev:blastworker dev:downloadworker",
    "dev:blastworker": "node -r dotenv/config worker/build/worker/blastworker.js dotenv_config_path=./.env.development",
    "dev:downloadworker": "node -r dotenv/config worker/build/worker/downloadworker.js dotenv_config_path=./.env.development",
    "start:worker": "run-p start:blastworker start:downloadworker",
    "start:blastworker": "node -r dotenv/config blastworker.js",
    "start:downloadworker": "node -r dotenv/config downloadworker.js",
    "lint": "next lint"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@prisma/client": "^5.0.0",
    "@react-hook/window-size": "^3.1.1",
    "@types/d3": "^7.4.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.14.10",
    "@types/object-hash": "^3.0.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.3.0",
    "bullmq": "^5.9.0",
    "bulma": "^1.0.1",
    "d3": "^7.8.5",
    "dotenv": "^16.3.1",
    "eslint": "^9.15.0",
    "eslint-config-next": "^14.2.5",
    "lodash": "^4.17.21",
    "next": "^14.2.5",
    "object-hash": "^3.0.0",
    "prisma": "^5.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.45.2",
    "react-select": "^5.7.4",
    "sass": "^1.63.6",
    "swr": "^2.2.0",
    "typescript": "^5.1.6",
    "xml-js": "^1.6.11",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/wicg-file-system-access": "^2023.10.5",
    "file-saver": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "supports-color": "^9.4.0"
  }
}
