    "@types/object-hash": "^3.0.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "bullmq": "^3.10.1",
    "bulma": "^0.9.4",
    "d3": "^7.8.3",
    "dotenv": "^16.0.3",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "lodash": "^4.17.21",
    "next": "13.2.3",
    "node-sass": "^8.0.0",
    "object-hash": "^3.0.0",
