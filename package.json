{
  "name": "blastserver",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:app": "next dev",
    "dev:worker": "npx tsc -p tsconfig.worker.json --watch",
    "build:app": "rm -rf .next && next build",
